// Modal
#ModalChooseT.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', style='display: none;', aria-hidden='true')
  .modal-dialog(role='document')
    .div(style='\
      position: relative;\
      display: -ms-flexbox;\
      display: flex;\
      -ms-flex-direction: column;\
      flex-direction: column;\
      width: 100%;\
      pointer-events: auto;\
      background-color: #fff;\
      background-clip: padding-box;\
      border: 1px solid rgba(0,0,0,.2);\
      border-radius: .3rem;\
      outline: 0;')
      .modal-header
        h5#exampleModalLabel.modal-title Choose a part
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') Ã—
      .col-md-12(style='padding: 39px; padding-bottom: 17px; margin-top: -21px;')
        .pricing-1.boxed.boxed--border(style='\
        margin-bottom: 0;\
        height: 421px; padding: 0%;')
          div(style='margin: 0px 10px;')
            img(alt='logo', src='/images/terminatorIcon.png', style='margin-left: auto; margin-right: auto; display: block; height: 50px;margin-bottom: 0px;')
            hr(style='padding-bottom:15px')
          div(style='margin: 0px 10px;')
            input.ng-valid.ng-touched.ng-dirty.ng-valid-parse.ng-empty.ng-pristine.ng-untouched(type='text', placeholder='Search...', ng-change='searchTerminator()', ng-model='searchValueTerminator', style='padding-right: 50%;')
          div(style='\
          height: 100%;margin: 10px 10px;')
            nav(style='\
            overflow-y: scroll; overflow-x: hidden; height: 294px;')
              ul(style='\
              margin-bottom: 0px;padding-top: 17px;')
                form.ng-pristine.ng-valid
                  .row.mb20
                    .col-md-12(ng-repeat="p in terminators")
                      .input-radio
                        input(ng-checked="terminator.name == p.name" ng-click="changeelementTerminator(p)", type='radio', name='radio', value='{{p.name}}' id='{{p.name}}')
                        label(for='{{p.name}}')
                      span.radiolabel {{p.name}} ({{p.quantity}}  &micro;l)
                      i.fas.fa-info-circle(style='font-size: 18px; right: 19px;color: #ffc600;position: absolute;margin-top: 2%;margin-left: 9%; cursor:pointer' data-toggle='modal', data-target='#ModalInfoTerminator' ng-click='modalTerminator(p._id)')
                        div(style=' font-size: 15px; font-weight: 100;right: 19px;color: #353535;position: absolute;margin-left: 9%; cursor:pointer')
                          include ./../../new-job/sections/modal_info_terminator.jade
              
      .modal-footer(style='justify-content: center')
                  button.btn.btn--primary.type--uppercase(type='button', data-dismiss='modal', style='color: white; font-weight: 900; min-width: 200px;', data-target='#ModalAddTerminator', data-toggle='modal') + New Part
                  button.btn.btn--primary.type--uppercase(type='button', data-dismiss='modal', ng-click='chooseTerminator()', style='color: white; font-weight: 900; min-width: 200px;') Choose



